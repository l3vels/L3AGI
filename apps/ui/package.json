    "test": "vite test",
    "eject": "vite eject",
    "lint": "eslint --ext .ts,.tsx src",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "postbuild": "rimraf ./build/**/*.js.map",
    "analyze": "source-map-explorer --no-border-checks dist/assets/*.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
